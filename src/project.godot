; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "StateMachine",
"class": "SpaceshipStateMachine",
"language": "GDScript",
"path": "res://scenes/ship/SpaceshipStateMachine.gd"
}, {
"base": "Node",
"class": "State",
"language": "GDScript",
"path": "res://scenes/state_machine/state.gd"
}, {
"base": "Reference",
"class": "StateMachine",
"language": "GDScript",
"path": "res://scenes/state_machine/state_machine.gd"
} ]
_global_script_class_icons={
"SpaceshipStateMachine": "",
"State": "",
"StateMachine": ""
}

[application]

config/name="Space Shooter"
run/main_scene="res://scenes/game/game.tscn"
config/icon="res://icon.png"

[display]

window/size/width=360
window/size/height=720
window/handheld/orientation="portrait"
window/stretch/mode="2d"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true
2d/default_gravity=0
2d/default_gravity_vector=Vector2( 0, 0 )

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
